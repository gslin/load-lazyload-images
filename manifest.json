{
    "manifest_version": 2,
    "name": "Load Lazyload Images",
    "version": "0.2021.0901.0",
    "description": "Load lazyload images automatically.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["load-lazyload-images.js"]
        }
    ],
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
